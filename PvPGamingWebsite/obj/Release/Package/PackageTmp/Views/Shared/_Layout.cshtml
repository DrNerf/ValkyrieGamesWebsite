<!doctype html>
<html lang="en-US">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<meta charset="utf-8">
		<title>@ViewBag.Title</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="description" content="">
		<meta name="keywords" content="">
		<!-- CSS -->
        <link href="~/Content/pnotify.custom.min.css" rel="stylesheet" />
		<link rel="shortcut icon" href="~/Content/design/img/favicon.png">
		<link rel="stylesheet" href="~/Content/design/style.css">
		<link rel="stylesheet" id="custom-style-css"  href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700italic,700,800,800italic" type="text/css" media="all" />
        @RenderSection("head", false)
	</head>
	<body class="home page page-id-26 page-template page-template-tmp-no-title-php">
		<div id="main_wrapper">
			<div class="container logo">
			<!-- Logo -->
			<a class="brand" href="/Home/Index">
                <img src="~/Images/VGLogo.png" alt="" style="width:150px; height:150px;" />
			</a>
			<!-- End Logo -->
			<div class="clear"></div>
             @RenderSection("featured", false)
		</div>
		<!-- NAVBAR -->
		<div class="navbar navbar-inverse container">
				<div class="navbar-inner">
					<button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					</button>
					<div class="nav-collapse collapse">
						<div class="menu-main-container">
							<ul class="nav">
								<li>
									<a href="/Home/Index">Home</a>
								</li>
								<li>
									<a href="/Forums/Index">Forums</a>
								</li>
								<li>
									<a href="/Chat/Index">Chat room</a>
								</li>
								<li>
                                    <a href="/Home/Projects">Projects</a>
								</li>
								<li>
									<a href="/Home/Team">The team</a>
								</li>
								<li>
                                    <a href="/Home/Ticket">Contact</a>
								</li>
                                @if(Request.IsAuthenticated && User.IsInRole("Moderator"))
                                {
								    <li class="dropdown">
									    <a href="#">Administration</a>
									    <ul class="sub-menu dropdown-menu">
										    <li ><a href="/Administration/NewsPosts">News</a></li>
										    <li><a href="/Administration/RolesPanel">Users roles</a></li>
										    <li><a href="/Administration/TicketsPanel">Tickes</a></li>
										    <li><a href="/Administration/ForumPanel">Forums</a></li>
										    <li><a href="/Administration/ProjectsStatus">Projects status</a></li>
										    <li><a href="/Administration/ProjectsPanel">Projects</a></li>
										    <li><a href="/Administration/Team">Team</a></li>
									    </ul>
								    </li>
                                }
							</ul>
						</div>
                        <div id="LoginPartialContainer">
                            @Html.Partial("_LoginPartial")
                        </div>
						<form method="get" id="header-searchform" action="http://skywarriorthemes.com/gameaddict/">
							<input autocomplete="off" value="" name="s" id="header-s" type="text">
							<input id="header-searchsubmit" value="Search" type="submit">
							
						</form>
					</div>
					<!--/.nav-collapse -->
				</div>
				<!-- /.navbar-inner -->
			</div>
		<div id="myModalL" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
				<h3>Login</h3>
			</div>
			<div class="modal-body">
				<div id="LoginWithAjax" class="default">
					<span id="LoginWithAjax_Status"></span>
					<form name="LoginWithAjax_Form" id="LoginWithAjax_Form" action="" method="post">
						<table cellpadding="0" cellspacing="0" width="100%">
							<tbody>
								<tr id="LoginWithAjax_Username">
									<td class="username_input">
										<input name="UserName" placeholder="Username" id="lwa_user_login" class="input" value="" type="text">
									</td>
								</tr>
								<tr id="LoginWithAjax_Password">
									<td class="password_input">
										<input placeholder="Password" name="Password" id="lwa_user_pass" class="input" value="" type="password">
									</td>
								</tr>
								<tr>
									<td colspan="2"></td>
								</tr>
								<tr id="LoginWithAjax_Submit">
									<td id="LoginWithAjax_SubmitButton">
										<input name="RememberMe" id="lwa_rememberme" value="forever" type="checkbox"> <label>Remember Me</label>
										<br><br>
										<input id="LoginAjaxBtn" class="button-small" value="Log In" type="button">
										<a class="reg-btn button-small" href="/Account/Register">Register</a>
										<input name="redirect_to" value="#" type="hidden">
										<input name="testcookie" value="1" type="hidden">
										<input name="lwa_profile_link" value="" type="hidden">
									</td>
								</tr>
							</tbody>
						</table>
					</form>
				</div>
			</div>
		</div>
		<div class="container normal-page">
			<div class="row">
				<div class="span12">
					<div class="template-wrapper">
						<div id="BodyContainer" class="block span12 first cf">
							<div id="block-2" class="block block-news_blog_style_block span12 first cf">
								@RenderBody()
							</div>
						</div>
						@RenderSection("sidebar", false)
					</div>
				</div>
			</div>
		</div>
		<footer class="container">
			<div class="span12">
                <div class="footer_widget span3">
					<h3>News</h3>
                    <script id="NewsFooterTemplate" type="text/x-jQuery-tmpl">
						<li>
                            <i class="icon-circle"></i> ${Title}
                            <br />
						    <small><i class="icon-calendar"></i> ${formatDate(PostDate)}</small>
						</li>
                    </script>
					<ul id="NewsFooter">
					</ul>
				</div>

				<div class="footer_widget span3">
					<h3>Latest topics</h3>
                    <script id="TopicsFooterTemplate" type="text/x-jQuery-tmpl">
                        <li>
							<div class="info">
								<a href="/Forums/Topic/${Id}"><b style="font-size:16px;"><i class="icon-comments"></i> ${Name}</b></a><br>
								<small><i class="icon-calendar"></i> ${formatDate(DateTimeCreated)} - <i class="icon-comment"></i> ${TopicPosts.length} Comments</small><br>
							</div>
							<div class="clear"></div>
						</li>
                    </script>
					<ul class="review" id="TopicsFooter">
						
					</ul>
				</div>
				<div class="footer_widget span3">
					<h3>From the forum</h3>
                    <script id="PostsFooterTemplate" type="text/x-jQuery-tmpl">
						<li>
                            <i class="icon-comment"></i> ${Poster} - ${Body}
                            <br />
						    <small><i class="icon-calendar"></i> ${formatDate(PostDateTime)}</small>
						</li>
                    </script>
					<ul id="PostsFooter">
					</ul>
				</div>
				<div class="widget-4 last footer_widget span3">
					<h3>latest works</h3>
                    <script id="ProjectsFooterTemplate" type="text/x-jQuery-tmpl">
						<a href="/Home/Project/${Id}" data-toggle="tooltip" data-original-title="${Title}">
                            {{if ThumbnailURL}}
							    <img src="${ThumbnailURL}" class="attachment-post-thumbnail wp-post-image" alt="7" onerror="jQuery(this).attr('src', '@(Url.Content("~/Content/design/img/defaults/305x305.jpg"))');">
                            {{else}}
							    <img src="@Url.Content("~/Content/design/img/defaults/305x305.jpg")" class="attachment-post-thumbnail wp-post-image" alt="7">
                            {{/if}}
						</a>
                    </script>
                    <div class="wcontent wprojects" id="ProjectsFooter">

                    </div>
				</div>
			</div>
			<div class="copyright span12">
				<p>© &nbsp;Design made by Skywarrior Themes.&nbsp;</p>
                <p style="float:right; margin-right:15px;">BETA</p>
			</div>
		</footer>
		<!-- JavaScript -->
		@*<script type="text/javascript" src="~/Content/design/js/jquery.js"></script>*@
        <script src="~/Scripts/jquery-1.9.1.min.js"></script>
        @Scripts.Render("~/bundles/modernizr")
        @Scripts.Render("~/bundles/jqueryval")
		<script type="text/javascript" src="~/Content/design/js/jquery.cookie.pack.js"></script>
		<script type="text/javascript" src="~/Content/design/js/jquery-migrate.min.js"></script>
		<script type="text/javascript" src="~/Content/design/js/jquery.fancybox.js"></script>
		<script type="text/javascript" src="~/Content/design/js/jquery.elastic.source.js"></script>
		<script type="text/javascript" src="~/Content/design/js/jquery.carouFredSel-6.2.1-packed.js"></script>
		<script type="text/javascript" src="~/Content/design/js/jquery-ui-1.10.3.custom.min.js"></script>
		<script type="text/javascript" src="~/Content/design/js/jquery.ui.totop.js"></script>
		<script type="text/javascript" src="~/Content/design/js/login-with-ajax.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-button.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-carousel.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-collapse.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-modal.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-tab.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-tooltip.js"></script>
		<script type="text/javascript" src="~/Content/design/js/bootstrap-transition.js"></script>
		<script type="text/javascript" src="~/Content/design/js/easing.js"></script>
		<script type="text/javascript" src="~/Content/design/js/global.js"></script>
		<script type="text/javascript" src="~/Content/design/js/imagescale.js"></script>
		<script type="text/javascript" src="~/Content/design/js/login-with-ajax.source.js"></script>
		<script type="text/javascript" src="~/Content/design/js/main.js"></script>
		<script type="text/javascript" src="~/Content/design/js/parallax.js"></script>
		<script type="text/javascript" src="~/Content/design/js/theme.min.js"></script>
        <script type="text/javascript" src="~/Content/design/js/tabs.js"></script>
        <script src="~/Scripts/pnotify.custom.min.js"></script>
        <script src="~/Scripts/LoginAjax.js"></script>
        <script src="~/Scripts/jQueryTmpl.js"></script>
        <script src="~/Scripts/FooterLoader.js"></script>
        @RenderSection("scripts", false)
		<!-- End JavaScript -->
	</body>
</html>